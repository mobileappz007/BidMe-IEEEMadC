                                             <div id="page-wrapper">
            <div class="container-fluid">
                <div class="row bg-title">
                    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
                     
                    <!-- /.col-lg-12 -->
                </div>
                <!-- .row -->
             
                   
                                  <div class="row">
                    <div class="col-sm-12">
                        <div class="white-box">
                            <h3 class="box-title">Subscription History </h3>
                          
                            <div class="table-responsive">
                                <table class="table">
                                               
                                        <thead>
                                            <tr>
                                                <th>S.No.</th>
                                                <th>Invoice Id</th>
                                                <th>Package Name</th>
                                                <th>Customer Name</th> 
                                                <th>Start Date</th>
                                                <th>End Date</th>
                                                <th>Total Price</th> 
                                                <th>Tax</th>
                                                <th>Discount</th>   
                                                <th>Final Price</th>
                                                <th>Currency Name</th>
                                          </tr>
                                        </thead>
                                        <tbody>
                                <% result.forEach(function(item, i){ %>
                <tr>
                <td><%= i+1 %></td>
                <td><%= item.invoice_id %></td>
                <td><%= item.package_name %></td>
                <td><%= item.name %></td>
                <td><%= item.start_date %></td>
                <td><%= item.end_date %></td>
                <td><%= item.total_price %></td>
                <td><%= item.tax %></td>
                <td><%= item.discount %></td>
                <td><%= item.final_price %></td>
                 <td><%= item.currency_type %></td>
            </tr>
            <div class="modal fade" id="delModal">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h2>Delete User</h2>
                </div>
                <div class="modal-body">
                    <p>Are You sure want to delete this Product <%= item.title %> !</p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-danger" id="del_btn" data-dismiss="modal" onclick="callHref()">Delete</button>
                    <button class="btn btn-default" data-dismiss="modal">Cancle</button>
                </div>
            </div>
        </div>
    </div>
        <% }) %>
                                        </tbody>
                                    </table>
                                </div>
                           </div>
                         </div>
                       </div>
                </div>

                <!-- /.row -->
                <!-- ============================================================== -->
