              <div class="row">
                    <div class="col-sm-12">
                        <div class="white-box">
                            <h3 class="box-title m-b-0">Add Subscription Package</h3>
         <form data-toggle="validator" action="/addSubs" method="post">                       
                              <div class="row">
                                                <div class="col-md-6">
                                
                                    <div class="form-group">
                                    <label for="inputName1" class="4u-label">Name</label>
                                    <input type="text" class="form-control" id="inputName1" name="packageName" placeholder="Enter Title" required> 

                                </div>
                            </div>
                                                <div class="col-md-6">
                                
                                <div class="form-group">
                                    <label  class="control-label">Auction_count</label>
                                    <input type="text" name="auctionCount" class="form-control" placeholder="Enter Auction">  <span class="help-block with-errors">how many auction can add in this subcription?</span>

                                     </div>
                                    </div>
                                </div>
                                 <div class="row">
                                                <div class="col-md-6">
                                
                                 <div class="form-group">
                                    <label  class="control-label">Days</label>
                                    <input type="text" class="form-control" id="inputEmail" name="totalDays" placeholder="Days" data-error="Please fill out this feild" required>
                                </div>
                            </div>
                                
                                                <div class="col-md-6">
                                
                                <div class="form-group">
                                    <label  class="control-label">Price</label>
                                    <input type="text" class="form-control" id="inputEmail" name="price" placeholder="price" data-error="Please fill out this feild" required>
                                </div>
                            </div>
                        </div>
                             <div class="row">
                                                <div class="col-md-6">
                                
                                <div class="form-group">
                                    <label>Currency</label>
                                    <select name="currency"  class="form-control" id="class" required> 
                                      <option>select</option>
                                      <% resultS.forEach(function(item, i){ %>
                            <option value= <%= item.curr_pub_id %> ><%= item.currency_type %></option>
                                      <% }) %>
                                   </select>
                               </div>
                           </div>
                       </div>

                                <div class="form-group">
                                    <button type="submit" class="btn btn-primary">Submit</button>
                                </div>
                            </form>
                        </div>
                    </div>

                </div>
                   
