                                  <div class="row">
                    <div class="col-sm-12">
                        <div class="white-box">
                            <h3 class="box-title">All Supports </h3>
                          
                            <div class="table-responsive">
                                <table class="table">
                                               
                                    <thead>
                                        <tr>
                                          
                                            <th>Title</th>
                                            <th>Description</th>
                                            <th>Status</th>
                                            <th>Action</th>
                                            
                                        </tr>
                                    </thead>
                                  
                                    <tbody id="student_table">
                                    	  <% result.forEach(function(item, i){ %>
                                        <tr>
                                            <td><%= item.title %></td>
                                            <td><%= item.description %></td>
                                              <% if ( item.status==0) { %>
                                                   <td>Pending</td>
                                            <% } else if ( item.status==1) { %>
                                                   <td>Apporved</td>
                                            <% } else { %>
                                                   <td>Reject</td>
                                            <% } %>     
                                  

                   <input type="hidden" name="id" value="<%= item.support_pub_id %>"/>             
                <td><button type="button" value="<%= item.support_pub_id %>" class="selected"  name="a" id="1" >Resolved</button><br><br>
                                           </td>
                                            
                                        </tr>

                                    <% }) %>
                                     </tbody>
                                    
                                   </table>
                                    
                               
                            </div>
</div>
</div>
</div>
                            </div>
                        </div>
                        <div>
                   <% if (pageCount > 1) { %>
            <ul class="pagination">
                <% if (currentPage > 1) { %>
                    <li><a href="/viewAllSupport/?page=1">&laquo;</a></li>
                <% } %>
                <% var i = 1;
                if (currentPage > 5) {
                    i = +currentPage - 4;
                } %>
                <% if (i !== 1) { %>
                    <li class="disabled"><a href="#">...</a></li>
                <% } %>
                <% for (i; i<=pageCount; i++) { %>
                    <% if (currentPage == i) { %>
                        <li class="active"><span><%= i %> <span class="sr-only">(current)</span></span></li>
                    <% } else { %>
                    <li><a href="/viewAllSupport/?page=<%= i %>"><%= i %></a></li>
                    <% } %>
                    <% if (i == (+currentPage + 4)) { %>
                        <li class="disabled"><a href="#">...</a></li>
                    <% break; } %>
                <% } %>
                <% if (currentPage != pageCount) { %>
                <li><a href="/viewAllSupport/?page=<%= pageCount %>">&raquo;</a></li>                <% } %>
            </ul>
        <% } %>
    </div>
                        
